backend index

{% if posts %}
<table>
<tr>
<th>Title</th>
<th>Published at</th>
</tr>
{% for post in posts %}
<tr>
<td>{{ post.title }}</td>
<td>{{ post.publishedAt.format('Y-m-d H:i:s') }}</td>
<td><a href="{% route 'balibali_blog_backend_edit' with ['id': post.id] %}">Edit</a></td>
</tr>
{% endfor %}
</table>
{% endif %}

<div>
<a href="{% route 'balibali_blog_backend_new' %}">New Post</a>
</div>
